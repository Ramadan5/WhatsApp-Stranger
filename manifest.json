{
    "short_name": "WhatsApp a Stranger",
    "name": "WhatsApp a Stranger",
    "description": "Chat instantly without saving the number!",
    "icons": [
        {
            "src": "Whatsapp-logo.png",
            "sizes": "144x144",
            "type": "image/png"
        }
    ],
    "start_url": "./",
    "scope": "./",
    "display": "standalone",
    "theme_color": "#00ff73",
    "background_color": "#00ff73",
    "share_target": {
        "action": "./script.js",
        "method": "POST",
        "enctype": "multiple/form-data",
        "params": {
            "title": "name",
            "text": "description",
            "url": "link",
            "files": [
                {
                    "name": "number",
                    "accept": "text/plain"
                }
            ]
        }
    }
}